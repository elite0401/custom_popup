.section-header{height:65px;}
.section-header>.grid--full>.grid__item{height:65px;}
.section-header>.grid--full>.grid__item.menu-cell{padding-top:9px;}
.section-header .section-header__cart-link{height:65px; line-height:48px;}
.section-header .user-account{line-height:48px;}
.footer-col__title{display:none;}
.section-footer .footer-col{padding:30px 20px 15px;}

.template-product .section-footer .section-footer__wrapper.mobile-hidden-product-cart,
.template-product .section-footer .section-footer__wrapper--bordered.mobile-hidden-product-cart,
.template-cart .section-footer .section-footer__wrapper.mobile-hidden-product-cart ,
.template-cart .section-footer .section-footer__wrapper--bordered.mobile-hidden-product-cart {
  display:block !important;
}

body.fixed-header--all .main-content, body.fixed-header--desktop .main-content {padding-top:105px !important;}


/* extra CSS Start */
.section-icon-list .section-icon-list--alt{background: rgb(246,238,215); background: -moz-linear-gradient(top, rgba(246,238,215,1) 0%, rgba(253,250,245,1) 100%, rgba(41,137,216,1) 100%, rgba(32,124,202,1) 100%, rgba(125,185,232,1) 100%, rgba(32,124,202,1) 100%); background: -webkit-linear-gradient(top, rgba(246,238,215,1) 0%,rgba(253,250,245,1) 100%,rgba(41,137,216,1) 100%,rgba(32,124,202,1) 100%,rgba(125,185,232,1) 100%,rgba(32,124,202,1) 100%); background: linear-gradient(to bottom, rgba(246,238,215,1) 0%,rgba(253,250,245,1) 100%,rgba(41,137,216,1) 100%,rgba(32,124,202,1) 100%,rgba(125,185,232,1) 100%,rgba(32,124,202,1) 100%); filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#f6eed7', endColorstr='#207cca',GradientType=0 );}
.section-icon-list .icon-list__item .icon-list__text{color:#353534;}
.slideshow__text-content .slideshow__title.slideshow__title--large{font-size:70px; line-height:70px;}
.slideshow__text-content .slideshow__title.slideshow__title--large span{font-size:20px; line-height:70px; display:inline-block; vertical-align:middle;}
.slideshow__text-content .slideshow__title.slideshow__title--large em{font-style:normal; font-size:70px; color: #e91e2e; line-height:70px; display:inline-block; vertical-align:middle;}
.testimonial .inner-content p{margin-bottom:5px;}
.section-testimonials .section-testimonials--alt{background:url({{ 'testi_bg_img.jpg' | asset_url }});}
.section-topbar .section-topbar__message{padding-left:15px;}
.section-header .cart-cell .icon-cart .icon-cart__badge{top:0;}
.first_loop_menu_color > a, .first_loop_menu_color > span{color:#e82332 !important;}
.loox-rating{color:#cdad60;}
/* extra CSS End */

/* Cart CSS Start */
.cart_template_section{width:100%; float:left; background:#f5f6f7;}
.custom_container{width:100%; max-width:1500px; margin:0 auto; display:block;}
.cart_top_text{width:100%; float:left; padding:30px 0;}
.cart_top_text .section-heading{width:50%; float:left; text-align:left; margin:0; letter-spacing:0;}
.cart_top_text p{width:50%; float:left; text-align:right; margin:0; color:#34393e;}
.cart_top_text p > span{color:#c82430; padding-left:10px;}
.cart_top_text p span{color:#e82332;}
.cart_form_cls{width:100%; float:left;}
ul.cart_listing_ul{width:100%; float:left; list-style:none; margin:0;}
ul.cart_listing_ul li.cart_main_li{width:100%; display:table; margin:0; padding-bottom:15px;}
.desktop_dp_cls{width:100%; float:none; display:inherit;}
.img_main_div{width:15%; display:table-cell; vertical-align:middle; margin-right:10px; background:#ffffff;}
.price_main_div{width:17%; display:table-cell; vertical-align:middle; margin-left:10px; background:#ffffff; text-align:right;}
.title_main_div{width:68%; position: relative; display:table-cell; vertical-align:middle; margin:0 5px; background:#ffffff;}
.title_main_div:before, .title_main_div:after{content:''; width:15px; height:100%; display:inline-block; position:absolute; left:0; top:0; background:#f5f6f7;}
.title_main_div:after{right:0; left:auto;}
.inner_content_cls{width:100%; display:inline-block; padding:15px 35px;}
.img_main_div figure{width:100%; display:inline-block; margin:0; text-align:center;}
.img_main_div figure a{width:auto; display:inline-block;}
.img_main_div figure img{width:auto; max-width:100%;}
.price_main_div p{margin-bottom:5px;}
.price_main_div p.price_top_text{color:#000; font-size:10px; line-height:15px;}
.price_main_div p.cart_qty_text{color:#a3a3a3; font-size:14px; font-weight:500; line-height:15px;}
.price_main_div p.cart_line_price .cart_item_orig_price{color:#34393e; font-size:18px; padding-right:10px; display:inline-block; vertical-align:middle;}
.price_main_div p.cart_line_price .cart_regular_price{color:#e82332; font-size:24px; font-weight:700; display:inline-block; vertical-align:middle;}
.product_title_cls{width:60%; float:left;}
.cart-table__item-controls{width:40%; float:right; text-align:right;}
.cart_title_left{width:auto; display:inline-block; vertical-align:middle;}
.cart_discount_price{width:50px; height:50px; border-radius:50%; color:#fff; background:#878787; margin-left:18px; font-size:16px; line-height:50px; display:inline-block; vertical-align:middle; text-align:center;}
.cart_discount_price em{font-style:normal;}
.cart-table__item-controls .js-qty{float:none; display:inline-block; vertical-align:middle; border:1px solid #9a9ca5; text-align:center; width:90px !important;}
.cart-table__item-controls .js-qty .js-qty__adjust{border:none; height:30px; width:20px; font-size:12px; color:#9a9ca5;}
.cart-table__item-controls .js-qty .js-qty__num{border:1px solid #9a9ca5; border-top:none; border-bottom:none; border-radius:0; box-sizing:border-box; height:40px; margin:0; text-align:center; width:40px; color:#9a9ca5;}
.cart_update_btn_cls{float:none; display:inline-block; vertical-align:middle; padding:12px 15px; margin-left:15px; font-size:12px; color:#9a9ca5; border:1px solid #9a9ca5;}
.cart_bottom_cls{width:100%; float:left; padding:10px 0 30px;}
.cart_bottom_cls .cart_subtotla_cls{width:100%; display:inline-block; text-align:right;}
.cart_bottom_cls .cart_subtotla_cls p{color:#34393e; font-size:16px; padding:0 25px; margin-bottom:20px;}
.cart_bottom_cls .cart_subtotla_cls p span{color:#e82332; font-size:24px; font-weight:700; padding-left:10px;}
.cart_checkout_div{text-align:right;}
.cart_checkout_div .cart__checkout-btn{border-radius:3px; font-size:20px;}
.addition_payment_div{width:100%; display:inline-block; padding-top:20px;}
.addition_payment_div span{width:auto; display:inline-block; vertical-align:middle;}
.addition_payment_div .additional-checkout-buttons{width:auto; display:inline-block; vertical-align:middle;}
.payment_img_figure{width:100%; display:inline-block; text-align:right; margin:0;}
.payment_img_figure p{color:#878787; font-size:15px; margin:0; line-height:initial;}
.payment_img_figure img{position:relative; top:8px;}
.cart_bottom_banner_div{background:url({{ 'cart_banner_bg.jpg' | asset_url }}); width:100%; float:left; margin:15px 0 0; padding:60px 0 35px;}
.cart_bottom_banner_div .icon-list__text{font-size:25px; color:#ccac60;}
.price_inner_div{padding:15px 25px 15px 5px;}
.mobile_cart_price_div, .mobile_cart_update{display:none;}
#product-add-to-cart-bar .product-variants{display:none !important;}
.product-form__item--quantity label{float:left; line-height:50px; padding-right:15px;}
.product-template .product-form .product-form__item .js-qty{float:left;}
.product-form__item--quantity{width:100%; float:left;}
.product-form__item.selector-wrapper{width:100%; float:left; padding:10px 0 5px;}
.product-form__item.selector-wrapper select{width:100%;}

/* Cart CSS End */

/* Black Friday CSS End */
.topbar_text_cls{font-size:14px !important; font-weight:700; text-transform:uppercase;}
.topbar_text_cls span{color:#fff;}
.slideshow__text-content .slideshow__title.slideshow__title--large{text-transform:uppercase;}
.slideshow__text-content .slideshow__title.slideshow__title--large em{color:#cdad60; display:inline-block; vertical-align:top;}
/* Black Friday CSS End */

/* Bold Quantity Break CSS Start */
.shappify-qty-msg{padding:15px 0;}
.shappify-qty-msg .STP-tier-block{background-color:#fff;border-color:#E91E2E;border-width:1px;border-radius:0px;border-style:solid;padding:0;display:inline-block;text-align:left;margin:0 0;}
.shappify-qty-msg .STP-tier-block-message span{color:#e91e2e; position:relative; z-index:99; width:24px; height:24px; font-size:13px; text-align:center; vertical-align:middle; display:inline-block; margin-right:5px;}
.shappify-qty-msg .STP-tier-block-message span:before{content:''; background:#fff; width:16px; height:16px; display:inline-block; position:absolute; left:0; right:0; top:0; margin:0 auto; z-index:-1;}
.shappify-qty-msg .STP-tier-block-message span:after{content:''; width:0; height:0; border-style:solid; border-width:12px 12px 0 12px; border-color:#ffffff transparent transparent transparent; bottom:0px; left:0; right:0; position:absolute; margin:0 auto; z-index:-1;}
.shappify-qty-msg .STP-tier-block-message{background:#E91E2E;color:#fff;font-size:16px;font-weight:bold;font-style:normal;text-decoration:none;padding:5px 10px;}
.shappify-qty-msg .STP-tier-block-lines{margin:0;padding:5px 10px 10px 10px;line-height:28px;background-color:#fff !important;border-color:#E91E2E;border-width:0px;border-style:solid;color:#000000;font-size:14px;font-weight:normal;font-style:normal;text-decoration:none;}
.shappify-qty-msg .STP-tier-block-lines b{padding-right:3px;}
.shappify-qty-msg .STP-tier-block-lines span{padding-left:3px; color:#E91E2E; font-weight:700;}

#shappify-qty-cart-msg{width:100%; float:left; font-weight:700;}
#shappify-qty-cart-msg .shopify_savings_message_text{background:#fff !important; border:none !important; color:#E91E2E !important;}
#shappify-qty-cart-msg span{color:#fff; position:relative; z-index:99; width:24px; height:24px; font-size:13px; text-align:center; display:inline-block; vertical-align:middle; margin-right:5px;}
#shappify-qty-cart-msg span:before{content:''; background:#e91e2e; width:16px; height:16px; display:inline-block; position:absolute; left:0; right:0; top:0; margin:0 auto; z-index:-1;}
#shappify-qty-cart-msg span:after{content:''; width:0; height:0; border-style:solid; border-width:12px 12px 0 12px; border-color:#e91e2e transparent transparent transparent; bottom:0px; left:0; right:0; position:absolute; margin:0 auto; z-index:-1;}
#shappify-qty-cart-msg em{font-style:normal; color:#000; font-weight:400;}
#shappify-qty-cart-msg b{color:#000;}
/* Bold Quantity Break CSS End */

@media(max-width:1500px){
  .custom_container{max-width:inherit; padding:0 20px;}
}

@media(max-width:1281px){
  .section-header .cart-cell .icon-cart .icon-cart__badge{top:-8px;}
}

@media(max-width:850px){
  .cart_update_btn_cls{padding:12px 17px; margin-top:5px;}
  .img_inner_content{padding:5px;}
}

@media(max-width:767px){
  body.fixed-header--all .main-content, body.fixed-header--desktop .main-content{padding-top:70px !important;}
  .slideshow__text-content .slideshow__title.slideshow__title--large{font-size:50px; line-height:50px;}
  .slideshow__text-content .slideshow__title.slideshow__title--large span{font-size:20px; line-height:50px;}
  .slideshow__text-content .slideshow__title.slideshow__title--large em{font-size: 50px; color: #cdad60; line-height:50px;}
  .section-testimonials .section-testimonials--alt{margin:25px 0;}
  .addition_payment_div{text-align:center; padding-top:5px;}
  .addition_payment_div span{width:100%; float:none; display:inline-block; padding-bottom:8px;}
  .addition_payment_div .additional-checkout-buttons{width:100%; float:none; display:inline-block;}
  .addition_payment_div .additional-checkout-buttons button{margin-left:0; width:100%; padding:2px 15px !important;}
  .cart_checkout_div .cart__checkout-btn{padding:25px 10px;}
  .payment_img_figure{padding:20px 0;}
  .payment_img_figure img{width:100%;}
  .trust_cart_figure{margin:0; width:100%; display:inline-block;}
  .trust_cart_figure img{width:100%;}
}

@media(max-width:641px){
  .price_main_div, .desktop_cart_update{display:none;}
  .product_title_cls{width:100%;}
  .cart_top_text{padding:20px 0;}
  .cart_top_text .section-heading{width:100%; float:none; display:inline-block; text-align:center; margin-bottom:5px;}
  .cart_top_text p{width:100%; float:none; display:inline-block; text-align:center;}
  ul.cart_listing_ul li.cart_main_li{display:inline-block; padding-bottom:0; margin-bottom:20px; background:#fff;}
  .mobile_cart_update{display:inline-block;}
  .img_main_div{width:15%; display:inline-block; float:left; margin:0;}
  .title_main_div{width:85%; float:left; display:inline-block; margin:0;}
  .cart-table__item-controls{width:100%; padding:0 0 15px;}
  .title_main_div:before, .title_main_div:after{display:none;}
  .inner_content_cls{padding:15px 15px 0;}
  .cart_update_btn_cls{margin-top:0;}
  .cart_discount_price{width:40px; height:40px; margin-left:10px; font-size:14px; line-height:40px; float:right}
  .mobile_cart_price_div{width:100%; display:inline-block; text-align:right; background:#fff; padding:0 15px 15px;}
  .mobile_cart_price_div p{margin-bottom:0; display:inline-block; vertical-align:inherit;}
  .mobile_cart_price_div p.cart_qty_text{color:#a3a3a3; padding-right:20px; font-size:14px;}
  .mobile_cart_price_div p.cart_line_price .cart_item_orig_price{color:#34393e; font-size:18px; padding-right:10px;}
  .mobile_cart_price_div p.cart_line_price .cart_regular_price{color:#c82430; font-size:24px;font-weight:700;}
  .cart_bottom_banner_div{padding:50px 15px 25px;}
  .cart_bottom_banner_div .icon-list__item{padding-bottom:15px;}
  .cart_bottom_banner_div .icon-list__text{font-size:20px;}
  .section-topbar .section-topbar__message{font-size:14px;}
}

@media(max-width:481px){
  body.fixed-header--all .main-content, body.fixed-header--desktop .main-content{padding-top:56px !important;}
  .section-header{height:55px;}
  .section-header>.grid--full>.grid__item{height:55px;}
  .section-header .section-header__cart-link{height:55px; padding:8px 0;}
  .img_main_div{width:25%;}
  .title_main_div{width:75%;}
  .slideshow__button{margin:0 !important; display:inline-block; white-space:inherit; padding:15px 10px;}
  .slideshow__text-content{top:45%;}
  .section-header .cart-cell .section-header__cart-link{padding:10px 0;}
  .slideshow__text-content .slideshow__title.slideshow__title--large{font-size:34px; line-height:35px;}
  .slideshow__text-content .slideshow__title.slideshow__title--large em{font-size:34px; line-height:35px;}
  .slideshow__text-content .slideshow__title.slideshow__title--large span{font-size:14px; line-height:35px;}
}

@media(max-width:375px){
  .img_inner_content{padding:15px 0 15px 15px;}
  .mobile_cart_price_div p.cart_qty_text{padding-right:10px;}
  .mobile_cart_price_div p.cart_line_price .cart_item_orig_price{padding-right:5px;}
}